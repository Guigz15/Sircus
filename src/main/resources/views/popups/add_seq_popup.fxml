<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>
<AnchorPane xmlns:fx="http://javafx.com/fxml/1" prefHeight="160.0" prefWidth="500.0"
            xmlns="http://javafx.com/javafx/14.0.2.1">
    <GridPane layoutX="66.0" layoutY="-6.0" prefHeight="166.0" prefWidth="500.0" AnchorPane.bottomAnchor="0.0"
              AnchorPane.leftAnchor="0.0" AnchorPane.rightAnchor="0.0" AnchorPane.topAnchor="-6.0">
        <columnConstraints>
            <ColumnConstraints hgrow="SOMETIMES" maxWidth="243.0" minWidth="190.0" prefWidth="195.0"/>
            <ColumnConstraints hgrow="SOMETIMES" maxWidth="357.0" minWidth="100.0" prefWidth="285.0"/>
        </columnConstraints>
        <rowConstraints>
            <RowConstraints maxHeight="188.0" minHeight="30.0" prefHeight="43.0" vgrow="SOMETIMES"/>
            <RowConstraints maxHeight="195.0" minHeight="30.0" prefHeight="8.0" vgrow="SOMETIMES"/>
            <RowConstraints maxHeight="205.0" minHeight="30.0" prefHeight="31.0" vgrow="SOMETIMES"/>
        </rowConstraints>
        <padding>
            <Insets bottom="10.0" left="10.0" right="10.0" top="10.0"/>
        </padding>
        <TextField fx:id="nameNewSeq" prefHeight="25.0" prefWidth="386.0" GridPane.columnIndex="1"
                   GridPane.rowIndex="1"/>
        <HBox alignment="BOTTOM_RIGHT" prefHeight="100.0" prefWidth="479.0" spacing="20.0" GridPane.columnSpan="2"
              GridPane.rowIndex="2">
            <Button fx:id="addSeqCancel" alignment="CENTER" minHeight="27.0" minWidth="58.0" mnemonicParsing="false"
                    style="-fx-background-color: rgb(255,89,89);" text="Annuler" textFill="WHITE"/>
            <Button fx:id="addSeqSave" alignment="CENTER" minHeight="27.0" minWidth="58.0" mnemonicParsing="false"
                    style="-fx-background-color: #006380;" text="Ajouter" textFill="WHITE"/>
        </HBox>
        <RadioButton fx:id="addNewSeq" mnemonicParsing="false" text="Nouvelle séquence" GridPane.rowIndex="1"/>
        <RadioButton fx:id="addCopySeq" mnemonicParsing="false" prefHeight="17.0" prefWidth="196.0"
                     text="Copier une séquence existante"/>
        <ComboBox fx:id="nameListSeq" prefHeight="25.0" prefWidth="385.0" GridPane.columnIndex="1"/>
    </GridPane>
</AnchorPane>
