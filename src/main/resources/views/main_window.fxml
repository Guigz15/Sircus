                     <Insets right="15.0" />
                  </HBox.margin>
                  <Text strokeType="OUTSIDE" strokeWidth="0.0" text="Praticien">
                     <VBox.margin>
                        <Insets bottom="5.0" />
                     </VBox.margin>
                     <font>
                        <Font size="18.0" />
                     </font>
                  </Text>
                  <RadioButton mnemonicParsing="false" selected="true">
                     <toggleGroup>
                        <ToggleGroup fx:id="type" />
                     </toggleGroup>
                  </RadioButton>
               </VBox>
               <VBox alignment="TOP_CENTER" prefHeight="200.0" prefWidth="100.0">
                  <HBox.margin>
                     <Insets left="15.0" />
                  </HBox.margin>
                  <Text strokeType="OUTSIDE" strokeWidth="0.0" text="Expérimentateur">
                     <VBox.margin>
                        <Insets bottom="5.0" />
                     </VBox.margin>
                     <font>
                        <Font size="18.0" />
                     </font>
                  </Text>
                  <RadioButton mnemonicParsing="false" toggleGroup="$type" />
               </VBox>
            </HBox>
            <VBox prefHeight="40.0" prefWidth="329.0">
               <VBox.margin>
