<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.*?>
<?import javafx.scene.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.image.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.text.*?>
<?import org.kordamp.ikonli.javafx.*?>

<BorderPane minHeight="600.0" minWidth="1000.0" prefHeight="600.0" prefWidth="1000.0" xmlns="http://javafx.com/javafx/14.0.2.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="fr.polytech.sircus.controller.MainWindowController">
   <top>
      <BorderPane maxHeight="-Infinity" prefHeight="64.0" prefWidth="200.0" BorderPane.alignment="CENTER">
         <left>
            <ComboBox fx:id="eyeTracker" promptText="Sélectionner un eye tracker" BorderPane.alignment="CENTER">
               <BorderPane.margin>
                  <Insets left="30.0" />
               </BorderPane.margin>
            </ComboBox>
         </left>
         <right>
            <Button fx:id="admin" contentDisplay="CENTER" mnemonicParsing="false" prefHeight="40.0" prefWidth="40.0" onAction="#handleConnection" BorderPane.alignment="CENTER_RIGHT">
               <BorderPane.margin>
                  <Insets right="15.0" />
               </BorderPane.margin>
               <graphic>
                  <ImageView fitHeight="30.0" fitWidth="30.0" preserveRatio="true">
                     <Image url="@../images/private-account.png" />
                  </ImageView>
               </graphic>
            </Button>
         </right>
      </BorderPane>
   </top>
