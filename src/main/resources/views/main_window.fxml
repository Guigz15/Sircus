                     <HBox.margin>
                        <Insets right="1.0" />
                     </HBox.margin>
                  </Text>
                  <Text fill="RED" strokeType="OUTSIDE" strokeWidth="0.0" text="*">
                     <font>
                        <Font size="18.0" />
                     </font>
                  </Text>
               </HBox>
               <TextField fx:id="forename">
                  <font>
                     <Font size="14.0" />
                  </font>
               </TextField>
            </VBox>
            <VBox prefHeight="40.0" prefWidth="329.0">
               <VBox.margin>
                  <Insets bottom="30.0" />
               </VBox.margin>
               <HBox prefHeight="100.0" prefWidth="200.0">
                  <VBox.margin>
                     <Insets bottom="2.0" />
                  </VBox.margin>
                  <Text strokeType="OUTSIDE" strokeWidth="0.0" text="Lieu d'acquisition">
                     <font>
                        <Font size="18.0" />
                     </font>
                     <HBox.margin>
                        <Insets right="1.0" />
                     </HBox.margin>
                  </Text>
                  <Text fill="RED" strokeType="OUTSIDE" strokeWidth="0.0" text="*">
                     <font>
                        <Font size="18.0" />
                     </font>
                  </Text>
               </HBox>
               <HBox prefHeight="100.0" prefWidth="200.0">
                  <ComboBox fx:id="location" prefHeight="25.0" prefWidth="275.0" promptText="Sélectionner un lieu">
                     <HBox.margin>
                        <Insets right="16.0" />
                     </HBox.margin>
                  </ComboBox>
                  <Button fx:id="locationAdd" mnemonicParsing="false" onMouseClicked="#addLocation" prefHeight="25.0" prefWidth="39.0">
                     <graphic>
                        <FontIcon iconLiteral="fa-plus" iconSize="15" />
                     </graphic>
                  </Button>
               </HBox>
            </VBox>
            <VBox prefHeight="40.0" prefWidth="329.0">
               <Text strokeType="OUTSIDE" strokeWidth="0.0" text="Méthode de classification">
                  <font>
                     <Font size="18.0" />
