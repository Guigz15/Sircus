                  <RadioButton mnemonicParsing="false" text="F" toggleGroup="$sex">
                     <HBox.margin>
                        <Insets right="40.0" />
                     </HBox.margin>
                     <font>
                        <Font size="14.0" />
                     </font>
                  </RadioButton>
               </HBox>
            </VBox>
            <HBox prefHeight="40.0" prefWidth="329.0">
