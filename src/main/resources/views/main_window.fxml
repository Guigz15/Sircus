                     <HBox.margin>
                        <Insets right="1.0" />
                     </HBox.margin>
                  </Text>
                  <Text fill="RED" strokeType="OUTSIDE" strokeWidth="0.0" text="*">
                     <font>
                        <Font size="18.0" />
                     </font>
                  </Text>
               </HBox>
               <TextField fx:id="name">
                  <font>
                     <Font size="14.0" />
